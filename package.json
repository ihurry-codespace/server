{
  "name": "ihurry-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsnd --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "start": "NODE_ENV=production node dist/src/index.js",
    "build": "NODE_ENV=production rimraf dist && tsc --project tsconfig.production.json",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:staged": "jest --findRelatedTests ./src/**/*.ts --passWithNoTests",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:ci": "npm test -- --coverage",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "test:watch": "jest --watchAll",
    "postinstall": "husky install && husky add .husky/commit-msg \"npx --no-install commitlint --edit $1\" && husky add .husky/pre-commit \"npm run test:staged\"",
    "test:coverage": "jest --coverage",
    "up": "docker-compose up"
  },
  "author": "rafamaxber@gmail.com",
  "license": "UNLICENSED",
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.38",
    "@types/jest": "^26.0.20",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.28",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": "15.x"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "fast-glob": "^3.2.5",
    "fastify": "^3.14.1",
    "fastify-cors": "^5.2.0",
    "fastify-helmet": "^5.3.1",
    "middie": "^5.2.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  }
}
