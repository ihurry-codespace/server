{
  "name": "ihurry-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev": "DEBUG=i18n:* NODE_ENV=development tsnd --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "start": "NODE_ENV=production node dist/src/index.js",
    "build": "NODE_ENV=production rimraf dist && tsc --project tsconfig.production.json",
    "test": "NODE_ENV=test jest --passWithNoTests --runInBand --no-cache",
    "test:staged": "npm test --findRelatedTests ./src/**/*.ts --passWithNoTests -c jest-unit-config.js",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm run test-up && npm test -- --watch -c jest-integration-config.js && npm run test-down",
    "test:ci": "npm test -- --coverage",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "test:watch": "jest --watchAll",
    "postinstall": "husky install && husky add .husky/commit-msg \"npx --no-install commitlint --edit $1\" && husky add .husky/pre-commit \"npm run test:staged\"",
    "test:coverage": "jest --coverage",
    "up": "docker-compose up --build -d",
    "down": "docker-compose down --remove-orphans",
    "test-up": "docker-compose --file docker-compose.test.yml up --build -d --remove-orphans",
    "test-down": "docker-compose --file docker-compose.test.yml down --remove-orphans"
  },
  "author": "rafamaxber@gmail.com",
  "license": "UNLICENSED",
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.38",
    "@types/i18n": "^0.13.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.28",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "cardinal": "^2.1.1",
    "eslint": "^7.20.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "sqlite3": "^5.0.2",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": "15.x"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "dotenv": "^8.2.0",
    "fastify": "^3.14.1",
    "fastify-cors": "^5.2.0",
    "fastify-helmet": "^5.3.1",
    "i18n": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "middie": "^5.2.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  }
}
